{
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
    },
    "cSpell.words": [
        "bopomofo",
        "bsearch",
        "deno",
        "denoland",
        "dtolnay",
        "JAMO",
        "kangxi",
        "mkdist",
        "qijs",
    ],
    "prettier.enable": false,
    "editor.formatOnSave": false,
    // Auto fix
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.organizeImports": "never",
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "toml",
        "json",
    ],
}
